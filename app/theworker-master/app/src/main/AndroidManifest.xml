<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.decompany.theworker" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_w"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.decompany.theworker.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.decompany.theworker.LoginActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.decompany.theworker.SignupActivity" android:theme="@style/AppTheme.Dark" />
    </application>

</manifest>